        <div class="row">
            <div class="col-3"></div>
            <div class="col-6 indexTitle">
                <p>Items For Sale</p>
            </div>
            <div class="col-3"></div>
            {{#each items}}
                <div class="col-1"></div>
                <div class="col-3 indexText">
                    <a href="/item/:{{this.Product_Id}}" style="color:white;">
                    <p>Product Name: {{this.Product_Name}}</p></a>
                </div>
                <div class="col-3 indexText" style="color:white;">
                    <a href="/item/:{{this.Product_Id}}" style="color:white;">
                    <p>Description: {{this.Description}}</p></a>
                </div>
                <div class="col-1 indexText" style="color:white;">
                    <a href="/item/:{{this.Product_Id}}" style="color:white;">
                    <p>Category: {{this.Category}}</p></a>
                </div>
                <div class="col-1 indexText" style="color:white;">
                    <a href="/item/:{{this.Product_Id}}" style="color:white;">
                    <p class="price">Price: ${{this.Price}}</p></a>
                </div>
                <div class="col-2">
                    <a href="/item/:{{this.Product_Id}}" style="color:white;">
                    <img src="{{this.Picture}}" class="indexImage" style="max-width:40%;"></a>
                </div>
                <div class="col-1"></div>
                <br><br>
            {{/each}}
        </div>
        <br><br>